# API Keys for the People Research Agent
# Copy this file to .env and fill in your actual API keys

# Anthropic API key for Claude 3.5 Sonnet LLM
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Tavily API key for web search functionality
TAVILY_API_KEY=your_tavily_api_key_here

# LangSmith API key for tracing and monitoring (optional)
LANGSMITH_API_KEY=your_langsmith_api_key_here

# LangSmith configuration
LANGSMITH_TRACING=true
LANGSMITH_PROJECT=people-research-agent

# Configuration parameters (optional - defaults will be used if not set)
MAX_SEARCH_QUERIES=3
MAX_SEARCH_RESULTS=3
